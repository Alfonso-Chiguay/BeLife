<Window x:Class="BeLife.Vistas.Seguro_vida"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BeLife.Vistas"
        mc:Ignorable="d"
        Title="Seguro_vida" Height="704.992" Width="353.524">
    <Grid Margin="0,0,515,-186">
        <Label Content="CONTRATO DE VIDA" HorizontalAlignment="Left" Margin="86,21,-209,0" VerticalAlignment="Top" Width="123"/>
        <Label Content="Numero de contrato" HorizontalAlignment="Left" Margin="27,72,-150,0" VerticalAlignment="Top" Width="123"/>
        <Label Content="Nombre" HorizontalAlignment="Left" Margin="29,214,-86,0" VerticalAlignment="Top" Width="57"/>
        <Label Content="Apellidos" HorizontalAlignment="Left" Margin="191,214,-314,0" VerticalAlignment="Top" Width="123"/>
        <Label Content="Fecha de nacimiento" HorizontalAlignment="Left" Margin="29,301,-152,0" VerticalAlignment="Top" Width="123"/>
        <Label Content="Sexo" HorizontalAlignment="Left" Margin="29,409,-152,0" VerticalAlignment="Top" Width="123"/>
        <Label Content="Estado Civil" HorizontalAlignment="Left" Margin="191,409,-314,0" VerticalAlignment="Top" Width="123"/>
        <TextBox x:Name="txt_contrato" HorizontalAlignment="Left" Height="23" Margin="27,98,-163,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="136" GotFocus="txt_contrato_GotFocus" LostFocus="txt_contrato_LostFocus" Foreground="#FFAAA4A4" Text="yyyyMMddhhmmss"/>
        <Button Content="Buscar contrato" HorizontalAlignment="Left" Margin="211,98,-314,0" VerticalAlignment="Top" Width="103" Height="23"/>
        <TextBox x:Name="txt_nombre" HorizontalAlignment="Left" Height="23" Margin="29,253,-140,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="111" IsEnabled="False" Text="Ej: Juan Jose" Foreground="#FFAAA4A4"/>
        <TextBox x:Name="txt_apellidos" HorizontalAlignment="Left" Height="23" Margin="191,253,-302,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="111" IsEnabled="False" Text="Ej: Perez Gonzalez" Foreground="#FFAAA4A4"/>
        <DatePicker x:Name="dp_fechaNacimiento" HorizontalAlignment="Left" Margin="29,339,-177,0" VerticalAlignment="Top" Width="148" IsEnabled="False"/>
        <Button Content="Guardar" HorizontalAlignment="Left" Margin="241,508,-312,0" VerticalAlignment="Top" Width="71" Height="23"/>
        <TextBox x:Name="txt_rut" HorizontalAlignment="Left" Height="23" Margin="27,166,-138,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="111" LostFocus="txt_rut_LostFocus" GotFocus="txt_rut_GotFocus" Foreground="#FFAAA4A4" Text="Ej: 12345678"/>
        <TextBox x:Name="txt_dv" HorizontalAlignment="Left" Height="23" Margin="161,166,-193,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="32" Foreground="#FFAAA4A4" Text="N" GotFocus="txt_dv_GotFocus" LostFocus="txt_dv_LostFocus"/>
        <Label Content="-" HorizontalAlignment="Left" Margin="143,163,-172,0" VerticalAlignment="Top" Width="29"/>
        <Button Content="Buscar cliente" HorizontalAlignment="Left" Margin="211,163,-314,0" VerticalAlignment="Top" Width="103" Height="23"/>
        <Label Content="Rut" HorizontalAlignment="Left" Margin="27,132,-150,0" VerticalAlignment="Top" Width="123"/>
        <ComboBox x:Name="cb_sexo" HorizontalAlignment="Left" Margin="29,452,-118,0" VerticalAlignment="Top" Width="89"/>
        <ComboBox x:Name="cb_estadoCivil" HorizontalAlignment="Left" Margin="191,452,-280,0" VerticalAlignment="Top" Width="89"/>

    </Grid>
</Window>
